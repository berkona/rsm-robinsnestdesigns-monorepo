{
  "version": 2,
  "name": "robins-nest-designs",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+", "headers": { "cache-control": "immutable" } },
    { "src": "/product/(?<productId>[^/]+).*$", "dest": "/product?productId=$productId" },
    { "src": "/category/(?<categoryId>[^/]+).*$", "dest": "/category?categoryId=$categoryId" },
    { "src": "/googlea7696e4bbcfecd2f.html", "dest": "/static/googlea7696e4bbcfecd2f.html" },
    { "src": "/order/(?<orderId>[^/]+).*$", "dest": "/order?orderId=$orderId" }
  ]
}
